#define OperationName "#Operation Name#"
author = "[TWC] #Author Name#";

onLoadName = OperationName;
briefingName = OperationName;
loadScreen = "twc_ui\loading.paa";
disabledAI = 1;
enableDebugConsole = 1;
disableChannels[] = {0,2,,5};
respawnButton = 0;

